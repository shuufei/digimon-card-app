          }
        }
      },
      "tags": []
    },
    "vs-desktop-app-e2e": {
      "root": "apps/vs-desktop-app-e2e",
      "sourceRoot": "apps/vs-desktop-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
